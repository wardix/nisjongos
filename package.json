{
  "name": "nisjongos",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/main.ts",
    "build": "bun build --minify --outdir dist --outfile dist/main.js --target bun src/main.ts",
    "check": "biome check --write .",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@types/bun": "latest",
    "@types/ssh2": "^1.15.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "ip-cidr": "^4.0.2",
    "mysql2": "^3.14.1",
    "nats": "^2.29.3",
    "pino": "^9.6.0",
    "ssh2": "^1.17.0"
  }
}
